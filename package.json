  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "preview": "vite preview",
    "check": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:test": "cross-env NODE_ENV=test drizzle-kit migrate",
    "db:seed:test": "cross-env NODE_ENV=test tsx scripts/seed-test-db.ts",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:analyze": "tsx scripts/analyze-db.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:unit:coverage": "vitest run tests/unit --coverage --coverage.reportsDirectory=coverage/unit",
    "test:integration": "vitest run tests/integration",
    "test:integration:coverage": "vitest run tests/integration --coverage --coverage.reportsDirectory=coverage/integration",
    "test:components": "vitest run tests/components",
    "test:components:coverage": "vitest run tests/components --coverage --coverage.reportsDirectory=coverage/components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:components",
    "test:all:coverage": "npm run test:unit:coverage && npm run test:integration:coverage && npm run test:components:coverage",
    "coverage:merge": "tsx scripts/merge-coverage.ts",
    "coverage:summary": "tsx scripts/coverage-summary.ts",
    "coverage:check": "tsx scripts/check-coverage.ts",
    "coverage:check-thresholds": "tsx scripts/check-coverage-thresholds.ts",
    "coverage:badge": "tsx scripts/generate-coverage-badge.ts",
    "coverage:badge:update": "tsx scripts/update-coverage-badge.ts",
    "coverage:html": "nyc report --reporter=html --report-dir=coverage/html",
    "coverage:percentage": "tsx scripts/get-coverage-percentage.ts",
    "test:coverage:combined": "tsx scripts/combine-coverage.ts",
    "sanity:autotiling": "tsx scripts/sanity-check-autotiling.ts"
